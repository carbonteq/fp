{
  "$schema": "https://raw.githubusercontent.com/hougesen/mdsf/main/schemas/v0.11.1/mdsf.schema.json",
  "format_finished_document": false,
  "javascript_runtime": "node",
  "languages": {
    "css": ["oxfmt"],
    "html": ["biome:format"],
    "javascript": ["oxfmt"],
    "json": ["biome:format"],
    "shell": [["shfmt", "beautysh"]],
    "sh": [["shfmt", "beautysh"]],
    "typescript": ["oxfmt"],
    "ts": ["oxfmt"],
    "js": ["oxfmt"],
    "yaml": [["prettier", "yamlfmt", "yamlfix"]]
  }
}
