{
  "name": "@carbonteq/fp",
  "description": "FP utils",
  "version": "0.9.1",
  "author": "Arslan <rslnkrmt2552@gmail.com>",
  "license": "MIT",
  "scope": "@carbonteq",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbonteq/fp"
  },
  "bugs": {
    "url": "https://github.com/carbonteq/fp/issues"
  },
  "type": "module",
  "files": [
    "/dist",
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./result": {
      "require": {
        "types": "./dist/result.d.cts",
        "default": "./dist/result.cjs"
      },
      "import": {
        "types": "./dist/result.d.mts",
        "default": "./dist/result.mjs"
      }
    },
    "./option": {
      "require": {
        "types": "./dist/option.d.cts",
        "default": "./dist/option.cjs"
      },
      "import": {
        "types": "./dist/option.d.mts",
        "default": "./dist/option.mjs"
      }
    },
    "./match": {
      "require": {
        "types": "./dist/match.d.cts",
        "default": "./dist/match.cjs"
      },
      "import": {
        "types": "./dist/match.d.mts",
        "default": "./dist/match.mjs"
      }
    },
    "./errors": {
      "require": {
        "types": "./dist/errors.d.cts",
        "default": "./dist/errors.cjs"
      },
      "import": {
        "types": "./dist/errors.d.mts",
        "default": "./dist/errors.mjs"
      }
    }
  },
  "homepage": "https://github.com/carbonteq/fp#readme",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "biome check src tests",
    "lint:fix": "biome check src tests --write --unsafe",
    "fmt:dry": "biome format src tests",
    "fmt": "biome format src tests --write",
    "tc": "tsc --pretty --noEmit",
    "build": "NODE_ENV=production tsdown",
    "dev": "tsdown --watch",
    "test": "bun test",
    "test:node": "tsx --test tests/*.test.ts",
    "test:dbg": "bun test --inspect",
    "release": "changeset publish",
    "tag": "changeset tag",
    "knip": "knip"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@changesets/cli": "^2.29.8",
    "@microsoft/api-extractor": "^7.55.2",
    "@swc/core": "^1.15.8",
    "@tsconfig/bun": "^1.0.10",
    "@tsconfig/node22": "^22.0.5",
    "@types/bun": "^1.3.5",
    "@types/node": "^24.10.1",
    "@typescript/native-preview": "^7.0.0-dev.20260107.1",
    "bunup": "^0.16.17",
    "changesets": "^1.0.2",
    "jsr": "^0.13.5",
    "knip": "^5.80.0",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0",
    "oxlint-tsgolint": "^0.10.1",
    "tsdown": "^0.19.0-beta.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "trustedDependencies": [],
  "packageManager": "bun@1.3.5"
}
