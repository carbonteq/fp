{
  "name": "@carbonteq/fp",
  "description": "FP utils",
  "version": "0.9.1",
  "author": "Arslan <rslnkrmt2552@gmail.com>",
  "license": "MIT",
  "scope": "@carbonteq",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carbonteq/fp.git"
  },
  "bugs": {
    "url": "https://github.com/carbonteq/fp/issues"
  },
  "type": "module",
  "files": ["dist", "README.md", "examples"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./result": {
      "require": {
        "types": "./dist/result.d.cts",
        "default": "./dist/result.cjs"
      },
      "import": {
        "types": "./dist/result.d.mts",
        "default": "./dist/result.mjs"
      }
    },
    "./option": {
      "require": {
        "types": "./dist/option.d.cts",
        "default": "./dist/option.cjs"
      },
      "import": {
        "types": "./dist/option.d.mts",
        "default": "./dist/option.mjs"
      }
    },
    "./match": {
      "require": {
        "types": "./dist/match.d.cts",
        "default": "./dist/match.cjs"
      },
      "import": {
        "types": "./dist/match.d.mts",
        "default": "./dist/match.mjs"
      }
    },
    "./errors": {
      "require": {
        "types": "./dist/errors.d.cts",
        "default": "./dist/errors.cjs"
      },
      "import": {
        "types": "./dist/errors.d.mts",
        "default": "./dist/errors.mjs"
      }
    },
    "./flow": {
      "require": {
        "types": "./dist/flow.d.cts",
        "default": "./dist/flow.cjs"
      },
      "import": {
        "types": "./dist/flow.d.mts",
        "default": "./dist/flow.mjs"
      }
    },
    "./result-experimental": {
      "require": {
        "types": "./dist/result-experimental.d.cts",
        "default": "./dist/result-experimental.cjs"
      },
      "import": {
        "types": "./dist/result-experimental.d.mts",
        "default": "./dist/result-experimental.mjs"
      }
    },
    "./option-experimental": {
      "require": {
        "types": "./dist/option-experimental.d.cts",
        "default": "./dist/option-experimental.cjs"
      },
      "import": {
        "types": "./dist/option-experimental.d.mts",
        "default": "./dist/option-experimental.mjs"
      }
    }
  },
  "homepage": "https://github.com/carbonteq/fp#readme",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "biome check src tests",
    "lint:fix": "biome check src tests --write --unsafe",
    "olint": "oxlint --type-aware",
    "fmt:dry": "oxfmt --check .",
    "fmt": "oxfmt .",
    "tc": "tsc --pretty --noEmit",
    "typecheck": "tsgo --noEmit --pretty",
    "build": "NODE_ENV=production tsdown",
    "dev": "tsdown --watch",
    "test": "bun test",
    "test:dbg": "bun test --inspect",
    "release": "changeset publish",
    "tag": "changeset tag",
    "knip": "knip"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@changesets/cli": "^2.29.8",
    "@microsoft/api-extractor": "^7.56.0",
    "@swc/core": "^1.15.11",
    "@tsconfig/bun": "^1.0.10",
    "@tsconfig/node22": "^22.0.5",
    "@types/bun": "^1.3.8",
    "@types/node": "^24.10.1",
    "@typescript/native-preview": "^7.0.0-dev.20260131.1",
    "bunup": "^0.16.22",
    "changesets": "^1.0.2",
    "jsr": "^0.14.2",
    "knip": "^5.82.1",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "oxlint-tsgolint": "^0.11.4",
    "tsdown": "^0.20.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "trustedDependencies": [],
  "packageManager": "bun@1.3.5"
}
