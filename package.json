{
  "name": "@carbonteq/fp",
  "description": "FP utils",
  "version": "0.8.2",
  "type": "module",
  "files": ["/dist", "dist"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./result": {
      "require": {
        "types": "./dist/result.d.cts",
        "default": "./dist/result.cjs"
      },
      "import": {
        "types": "./dist/result.d.ts",
        "default": "./dist/result.js"
      }
    },
    "./option": {
      "require": {
        "types": "./dist/option.d.cts",
        "default": "./dist/option.cjs"
      },
      "import": {
        "types": "./dist/option.d.ts",
        "default": "./dist/option.js"
      }
    },
    "./match": {
      "require": {
        "types": "./dist/match.d.cts",
        "default": "./dist/match.cjs"
      },
      "import": {
        "types": "./dist/match.d.ts",
        "default": "./dist/match.js"
      }
    },
    "./package.json": "./package.json"
  },
  "author": "Arslan <rslnkrmt2552@gmail.com>",
  "license": "MIT",
  "scope": "@carbonteq",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbonteq/fp"
  },
  "bugs": {
    "url": "https://github.com/carbonteq/fp/issues"
  },
  "homepage": "https://github.com/carbonteq/fp#readme",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "biome check src tests",
    "lint:fix": "biome check src tests --write --unsafe",
    "fmt:dry": "biome format src tests",
    "fmt": "biome format src tests --write",
    "tc": "tsc --pretty --noEmit --project tsconfig.build.json",
    "build": "NODE_ENV=production tsdown",
    "bd": "bunup",
    "dev": "bunup --watch",
    "test": "bun test",
    "test:node": "tsx --test tests/*.test.ts",
    "test:dbg": "bun test --inspect",
    "release": "changeset publish",
    "tag": "changeset tag"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "@changesets/cli": "^2.29.7",
    "@microsoft/api-extractor": "^7.53.3",
    "@swc/core": "^1.13.5",
    "@tsconfig/node20": "^20.1.6",
    "@types/node": "^20.19.21",
    "@typescript/native-preview": "^7.0.0-dev.20251025.1",
    "bunup": "^0.15.7",
    "changesets": "^1.0.2",
    "jsr": "^0.13.5",
    "oxlint": "^1.24.0",
    "oxlint-tsgolint": "^0.3.0",
    "tsdown": "^0.15.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "trustedDependencies": ["@swc/core", "esbuild"]
}
