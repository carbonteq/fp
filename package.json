{
  "name": "@carbonteq/fp",
  "description": "FP utils",
  "version": "0.8.2",
  "author": "Arslan <rslnkrmt2552@gmail.com>",
  "license": "MIT",
  "scope": "@carbonteq",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbonteq/fp"
  },
  "bugs": {
    "url": "https://github.com/carbonteq/fp/issues"
  },
  "type": "module",
  "files": ["/dist", "dist"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./result": {
      "require": {
        "types": "./dist/result.d.cts",
        "default": "./dist/result.cjs"
      },
      "import": {
        "types": "./dist/result.d.ts",
        "default": "./dist/result.js"
      }
    },
    "./result.experimental": {
      "require": {
        "types": "./dist/result.experimental.d.cts",
        "default": "./dist/result.experimental.cjs"
      },
      "import": {
        "types": "./dist/result.experimental.d.ts",
        "default": "./dist/result.experimental.js"
      }
    },
    "./option": {
      "require": {
        "types": "./dist/option.d.cts",
        "default": "./dist/option.cjs"
      },
      "import": {
        "types": "./dist/option.d.ts",
        "default": "./dist/option.js"
      }
    },
    "./match": {
      "require": {
        "types": "./dist/match.d.cts",
        "default": "./dist/match.cjs"
      },
      "import": {
        "types": "./dist/match.d.ts",
        "default": "./dist/match.js"
      }
    },
    "./errors": {
      "require": {
        "types": "./dist/errors.d.cts",
        "default": "./dist/errors.cjs"
      },
      "import": {
        "types": "./dist/errors.d.ts",
        "default": "./dist/errors.js"
      }
    },
    "./either": {
      "require": {
        "types": "./dist/either.d.cts",
        "default": "./dist/either.cjs"
      },
      "import": {
        "types": "./dist/either.d.ts",
        "default": "./dist/either.js"
      }
    },
    "./package.json": "./package.json"
  },
  "homepage": "https://github.com/carbonteq/fp#readme",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "biome check src tests",
    "lint:fix": "biome check src tests --write --unsafe",
    "fmt:dry": "biome format src tests",
    "fmt": "biome format src tests --write",
    "tc": "tsc --pretty --noEmit",
    "build": "NODE_ENV=production bunup",
    "dev": "bunup --watch",
    "test": "bun test",
    "test:node": "tsx --test tests/*.test.ts",
    "test:dbg": "bun test --inspect",
    "release": "changeset publish",
    "tag": "changeset tag",
    "knip": "knip"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@changesets/cli": "^2.29.7",
    "@microsoft/api-extractor": "^7.55.0",
    "@swc/core": "^1.15.2",
    "@tsconfig/node22": "^22.0.4",
    "@types/bun": "^1.3.2",
    "@types/node": "^24.10.1",
    "@typescript/native-preview": "^7.0.0-dev.20251116.1",
    "bunup": "^0.15.14",
    "changesets": "^1.0.2",
    "jsr": "^0.13.5",
    "knip": "^5.69.1",
    "oxlint": "^1.28.0",
    "oxlint-tsgolint": "^0.6.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "trustedDependencies": [],
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
